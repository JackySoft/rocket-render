    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "10.1.0",
    "standard-version": "^9.5.0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2",
    "vuepress-plugin-auto-sidebar": "^2.3.2",
    "vuepress-plugin-demo-container": "^0.2.0",
    "axios": "^0.24.0",
    "core-js": "^3.19.0",
    "vue-router": "^3.5.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
